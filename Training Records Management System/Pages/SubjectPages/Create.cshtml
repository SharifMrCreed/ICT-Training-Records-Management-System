@page
@model Training_Records_Management_System.Pages.SubjectPages.CreateModel

@{
    ViewData["Title"] = "Create";
}
<div class="text-center h1">
     New Subject
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Subject.Title" class="control-label"></label>
                <input asp-for="Subject.Title" class="form-control" />
                <span asp-validation-for="Subject.Title" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group justify-content-center">
                
    <a class="btn btn-outline-primary" asp-page="Index">Back</a>
    <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div class = "justify-content-center">
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
